{
    "name": "ReactAnalytics",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "",
    "devtools_page": "devtool.html",
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "externally_connectable": {
        "matches": ["http://localhost:8080/*"]
    },
    "permissions": [
        "tabs",
        "notifications",
        "*://*/*"
    ]
}